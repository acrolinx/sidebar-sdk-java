		  <sthead> 
			 <stentry>Feature</stentry> 
			 <stentry><term>World Time Pro v2</term></stentry> 
			 <stentry><term>World Time Pro Deluxe Edition</term></stentry> 
		  </sthead> 
		  <strow> 
			 <stentry>Add, subtract, multiply and divide units of time in hours,
				minutes and seconds</stentry> 
			 <stentry>Yes</stentry> 
			 <stentry>Yes</stentry> 
		  </strow> 
		  <strow> 
			 <stentry><term>Fraction Converter</term></stentry> 
			 <stentry>Yes</stentry> 
			 <stentry>Yes</stentry> 
		  </strow> 
		  <strow> 
			 <stentry><term>Fraction Creator</term></stentry> 
			 <stentry>Yes</stentry> 
			 <stentry>Yes</stentry> 
		  </strow> 
		  <strow> 
			 <stentry><term>Date Analysis Engine</term></stentry> 
			 <stentry>Yes</stentry> 
			 <stentry>Yes</stentry> 
		  </strow> 
		  <strow> 
			 <stentry><term>Standard/Military Time Converter</term></stentry> 
			 <stentry>Yes</stentry> 
			 <stentry>Yes</stentry> 
		  </strow> 
		  <strow> 
			 <stentry>Memory (<uicontrol>M+</uicontrol>)</stentry> 
			 <stentry>No</stentry> 
			 <stentry>Yes</stentry> 
