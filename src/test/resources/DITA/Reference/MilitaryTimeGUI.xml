  <refbody> 
	 <section id="section_7276B18487AF4FEDA0E402F10C027D73"> 
		<title>The 
		  <wintitle>Standard/Military Time Converter
			 window</wintitle></title><image href="../Images/win/MilitaryTimeGUI.png"
		id="image_DCECCC134F314309AC111172B013363D" width="339" height="313"> 
		  <alt>This graphic shows the fields and buttons in the
			 .<wintitle>Standard/Military Time Converter window</wintitle> 
		  </alt> 
		</image> 
	 </section> 
	 <properties id="properties_2F9AC531BF4B4E6B8A24AD05DAA3BD39"> 
		<prophead> 
		  <proptypehd>Field or button</proptypehd> 
		  <propdeschd>Function</propdeschd> 
		</prophead> 
		<property> 
		  <proptype><uicontrol>? button</uicontrol></proptype> 
		  <propdesc>Opens the help.</propdesc> 
		</property> 
		<property> 
		  <proptype><uicontrol>Standard time converter</uicontrol></proptype> 
		  <propdesc>Specifies that the calculation converts 
			 <term>military time</term> to 
			 <term>standard time</term>.</propdesc> 
		</property> 
		<property> 
		  <proptype><uicontrol>Military time converter</uicontrol></proptype> 
		  <propdesc>Specifies that the calculation converts 
			 <term>standard time</term> to 
			 <term>military time</term>.</propdesc> 
		</property> 
		<property> 
		  <proptype><uicontrol>Military Time fields</uicontrol></proptype> 
		  <propdesc>If the 
			 <uicontrol>Standard time converter</uicontrol> is selected, specifies
			 the 
			 <term>military time</term> to be converted.</propdesc> 
		</property> 
		<property> 
		  <propdesc>If the 
			 <uicontrol>Military time converter</uicontrol> is selected, displays
			 the result of a 
			 <term>standard time</term> conversion.</propdesc> 
		</property> 
		<property> 
		  <proptype><uicontrol>Convert button</uicontrol></proptype> 
		  <propdesc>If the 
			 <uicontrol>Standard time converter</uicontrol> is selected, converts 
			 <term>military time</term> to 
			 <term>standard time</term>.</propdesc> 
		</property> 
		<property> 
		  <propdesc>If the 
			 <uicontrol>Military time converter</uicontrol> is selected, converts 
			 <term>standard time</term> to 
			 <term>military time</term>.</propdesc> 
		</property> 
		<property> 
		  <proptype><uicontrol>Standard Time fields</uicontrol></proptype> 
		  <propdesc>If the 
			 <uicontrol>Standard time converter</uicontrol> is selected, displays
			 the result of a 
			 <term>military time</term> conversion.</propdesc> 
		</property> 
		<property> 
		  <propdesc>If the 
			 <uicontrol>Military time converter</uicontrol> is selected, specifies
			 the 
			 <term>standard time</term> to be converted.</propdesc> 
		</property> 
		<property> 
		  <proptype><uicontrol>AM/PM drop-down box</uicontrol></proptype> 
		  <propdesc>Specifies if the 
			 <term>standard time</term> displayed is AM or PM.</propdesc> 
		</property> 
		<property> 
		  <proptype><uicontrol>Reset button</uicontrol></proptype> 
		  <propdesc>Deletes all values from the window and selects the 
			 <uicontrol>Standard time converter</uicontrol> if it is not already
			 selected.</propdesc> 
		</property> 
		<property> 
		  <proptype><uicontrol>Clear button</uicontrol></proptype> 
		  <propdesc>Deletes all vaules from the window.</propdesc> 
		</property> 
	 </properties> 
  </refbody>
