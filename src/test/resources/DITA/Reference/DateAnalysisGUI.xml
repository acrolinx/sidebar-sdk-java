  <refbody> 
	 <section id="section_E0998E7525D94B439272C73B4EDA574A"> 
		<title>The 
		  <wintitle>Date Analysis Engine window</wintitle></title><image
		href="../Images/win/DateAnalysisGUI.png"
		id="image_8B5FCAA9A1C6477CB1BE63D92A73A7F3" width="628" height="341"> 
		  <alt>This graphic shows the fields and buttons in the . The window
			 contains the and the .<wintitle>Date Analysis Engine
				window</wintitle><wintitle>Date Analysis Engine
				pane</wintitle><wintitle>calendar pane</wintitle> 
		  </alt> 
		</image> 
	 </section> 
	 <properties id="properties_F74BD6DB4F8E44119D2E84B9D1BCE8E2"> 
		<prophead> 
		  <proptypehd>Field or button</proptypehd> 
		  <propdeschd>Function</propdeschd> 
		</prophead> 
		<property> 
		  <proptype><uicontrol>Range selector</uicontrol></proptype> 
		  <propdesc>Inactive in this edition of 
			 <term>World Time Pro</term>.</propdesc> 
		</property> 
		<property> 
		  <proptype><uicontrol>Day drop-down box</uicontrol></proptype> 
		  <propdesc>Specifies the day that is used in the calculation.</propdesc>
		  
		</property> 
		<property> 
		  <proptype><uicontrol>Month drop-down box</uicontrol></proptype> 
		  <propdesc>Specifies the month that is used in the
			 calculation.</propdesc> 
		</property> 
		<property> 
		  <proptype><uicontrol>Year field</uicontrol></proptype> 
		  <propdesc>Specifies the year that is used in the
			 calculation.</propdesc> 
		</property> 
		<property> 
		  <proptype><uicontrol>Analyze button</uicontrol></proptype> 
		  <propdesc>Performs the calculation.</propdesc> 
		</property> 
		<property> 
		  <proptype><uicontrol>Day Of The Week field</uicontrol></proptype> 
		  <propdesc>Displays the day of the week for the date in the 
			 <uicontrol>Day</uicontrol>, 
			 <uicontrol>Month</uicontrol> and 
			 <uicontrol>Year</uicontrol> fields.</propdesc> 
		</property> 
		<property> 
		  <proptype><uicontrol>Display Calendar checkbox</uicontrol></proptype> 
		  <propdesc>Specifies if the 
			 <wintitle>calendar pane</wintitle> appears.</propdesc> 
		</property> 
		<property> 
		  <proptype><uicontrol>Reset button</uicontrol></proptype> 
		  <propdesc>Deletes the values from the 
			 <uicontrol>Year</uicontrol> and 
			 <uicontrol>Day Of The Week</uicontrol> fields, clears the 
			 <wintitle>calendar pane</wintitle>, changes the value of the 
			 <uicontrol>Day</uicontrol> field to 1, and changes the value of the 
			 <uicontrol>Month</uicontrol> field to January.</propdesc> 
		</property> 
		<property> 
		  <proptype><uicontrol>Clear button</uicontrol></proptype> 
		  <propdesc>Deletes the values from the 
			 <uicontrol>Year</uicontrol> and 
			 <uicontrol>Day Of The Week</uicontrol> fields, and clears the 
			 <wintitle>calendar pane</wintitle>.</propdesc> 
		</property> 
		<property> 
		  <proptype><uicontrol>Theoretical button</uicontrol></proptype> 
		  <propdesc>Inactive in this edition of 
			 <term>World Time Pro</term>.</propdesc> 
		</property> 
		<property> 
		  <proptype><uicontrol>Factual button</uicontrol></proptype> 
		  <propdesc>Inactive in this edition of 
			 <term>World Time Pro</term>.</propdesc> 
		</property> 
		<property> 
		  <proptype><wintitle>Calendar pane</wintitle></proptype> 
		  <propdesc>Displays the days of the week for the month specifed in the 
			 <uicontrol>Month</uicontrol> and 
			 <uicontrol>Year</uicontrol> fields.</propdesc> 
		</property> 
	 </properties> 
  </refbody>
